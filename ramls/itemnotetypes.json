{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "A collection of item note types",
  "type": "object",
  "properties": {
    "itemNoteTypes": {
      "description": "List of item note types",
      "id": "itemNoteType",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "itemnotetype.json"
      }
    },
    "totalRecords": {
      "description": "Estimated or exact total number of records",
      "type": "integer"
    }
  },
  "required": [
    "itemNoteTypes",
    "totalRecords"
  ]
}

