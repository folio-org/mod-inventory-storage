{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "A collection of Instance resource types (controlled vocabulary)",
  "type": "object",
  "properties": {
    "instanceTypes": {
      "description": "List of instance types",
      "id": "instanceType",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "instancetype.json"
      }
    },
    "totalRecords": {
      "description": "Estimated or exact total number of records",
      "type": "integer"
    }
  },
  "required": [
    "instanceTypes",
    "totalRecords"
  ]
}
