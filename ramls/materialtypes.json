{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "A collection of material types",
  "type": "object",
  "properties": {
    "mtypes": {
      "description": "List of material types",
      "id": "mtype",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "materialtype.json"
      }
    },
    "totalRecords": {
      "description": "Estimated or exact total number of records",
      "type": "integer"
    }
  },
  "required": [
    "mtypes",
    "totalRecords"
  ]
}
