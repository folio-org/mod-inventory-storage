{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "A collection of holdings types",
  "type": "object",
  "properties": {
    "holdingsTypes": {
      "description": "List of holdings types",
      "id": "holdingsType",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "holdingsType.json"
      }
    },
    "totalRecords": {
      "description": "Estimated or exact total number of records",
      "type": "integer"
    }
  },
  "required": [
    "holdingsTypes",
    "totalRecords"
  ]
}

