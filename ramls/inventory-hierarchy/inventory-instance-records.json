{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Response schema for Inventory items and holdings view",
  "type": "object",
  "properties": {
    "instanceId": {
      "description": "Inventory updated instance identifier",
      "type": "string",
      "$ref": "../uuid.json"
    },
    "source": {
      "description": "Source of metadata and format of the underlying record to the instance record",
      "type": "string"
    },
    "itemsAndHoldings": {
      "description": "Aggregated items and holdings fields",
      "type": "object",
      "$ref": "inventory-items-and-holdings.json"
    }
  },
  "required": [
    "instanceId",
    "source",
    "itemsAndHoldings"
  ],
  "additionalProperties": false
}
